            <input type="radio" value=1.00 asp-for="Review.Mechanics" disabled />1
           <input type="radio" value=2.00 asp-for="Review.Mechanics" disabled />2
           <input type="radio" value=3.00 asp-for="Review.Mechanics" disabled />3
           <input type="radio" value=4.00 asp-for="Review.Mechanics" disabled />4
           <input type="radio" value=5.00 asp-for="Review.Mechanics" disabled />5
           <span asp-validation-for="Review.Mechanics" class="text-danger"></span>
         </div>
         <div class="mb-3">
