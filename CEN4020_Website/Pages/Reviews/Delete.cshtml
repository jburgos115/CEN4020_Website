           <input asp-for="Review.PotentialForOralPresentationComments" type="text" size="50" disabled />
           <span asp-validation-for="Review.PotentialForOralPresentationComments" class="text-danger"></span>
         </div>
         <div class="mb-3">
           <label class="control-label col-sm-3">Overall Rating:</label>
            <input type="radio" value=1.00 asp-for="Review.OverallRating" disabled />1
           <input type="radio" value=2.00 asp-for="Review.OverallRating" disabled />2
           <input type="radio" value=3.00 asp-for="Review.OverallRating" disabled />3
           <input type="radio" value=4.00 asp-for="Review.OverallRating" disabled />4
           <input type="radio" value=5.00 asp-for="Review.OverallRating" disabled />5
           <span asp-validation-for="Review.OverallRating" class="text-danger"></span>
         </div>
         <div class="mb-3">
           <label class="control-label col-sm-3">Overall Rating Comments:</label>
           <input asp-for="Review.OverallRatingComments" type="text" size="50" disabled />
           <span asp-validation-for="Review.OverallRatingComments" class="text-danger"></span>
         </div>
         <div class="mb-3">
           <label class="control-label col-sm-3">Comfort Level Topic:</label>
            <input type="radio" value=1.00 asp-for="Review.ComfortLevelTopic" disabled />1
           <input type="radio" value=2.00 asp-for="Review.ComfortLevelTopic" disabled />2
           <input type="radio" value=3.00 asp-for="Review.ComfortLevelTopic" disabled />3
