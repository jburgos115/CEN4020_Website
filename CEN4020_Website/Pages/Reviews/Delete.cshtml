           <span asp-validation-for="Review.OrganizationOfPaper" class="text-danger"></span>
         </div>
         <div class="mb-3">
           <label class="control-label col-sm-3">Clarity Of Main Message:</label>
           <input type="radio" value=1.00 asp-for="Review.ClarityOfMainMessage" disabled />1
           <input type="radio" value=2.00 asp-for="Review.ClarityOfMainMessage" disabled />2
           <input type="radio" value=3.00 asp-for="Review.ClarityOfMainMessage" disabled />3
           <input type="radio" value=4.00 asp-for="Review.ClarityOfMainMessage" disabled />4
           <input type="radio" value=5.00 asp-for="Review.ClarityOfMainMessage" disabled />5
           <span asp-validation-for="Review.ClarityOfMainMessage" class="text-danger"></span>
