@page
@model CEN4020_Website.Pages.Reviews.DeleteModel
@{
}
<!--form used to delete a review-->
<form method="post">
	<input hidden asp-for="Review.PaperID" />
	<div class="border p-3 mt-4">
		<div class="row pb-2">
			<h2 class="text-primary pl-3">Delete Review</h2>
			<hr />
		</div>
		<div class="mb-3">
			<label asp-for="Review.PaperID">PaperID</label>
			<input asp-for="Review.PaperID" class="form-control" disabled />
		</div> 
		<div class="mb-3">
			<label asp-for="Review.ReviewerID">ReviewerID</label>
			<input asp-for="Review.ReviewerID" maxlength="3" class="form-control" disabled />
		</div>
		<div class="mb-3">
           <label class="control-label col-sm-3">Appropriateness Of Topic:</label>
           <input type="radio" value=1.00 asp-for="Review.AppropriatenessOfTopic" disabled />1
           <input type="radio" value=2.00 asp-for="Review.AppropriatenessOfTopic" disabled />2
           <input type="radio" value=3.00 asp-for="Review.AppropriatenessOfTopic" disabled />3
           <input type="radio" value=4.00 asp-for="Review.AppropriatenessOfTopic" disabled />4
           <input type="radio" value=5.00 asp-for="Review.AppropriatenessOfTopic" disabled />5
           <span asp-validation-for="Review.AppropriatenessOfTopic" class="text-danger"></span>
         </div>
          <div class="mb-3">
           <label class="control-label col-sm-3">Timeliness Of Topic:</label>
           <input type="radio" value=1.00 asp-for="Review.TimelinessOfTopic" disabled />1
           <input type="radio" value=2.00 asp-for="Review.TimelinessOfTopic" disabled />2
           <input type="radio" value=3.00 asp-for="Review.TimelinessOfTopic" disabled />3
           <input type="radio" value=4.00 asp-for="Review.TimelinessOfTopic" disabled />4
           <input type="radio" value=5.00 asp-for="Review.TimelinessOfTopic" disabled />5
           <span asp-validation-for="Review.TimelinessOfTopic" class="text-danger"></span>
         </div>
          <div class="mb-3">
           <label class="control-label col-sm-3">Supportive Evidence:</label>
           <input type="radio" value=1.00 asp-for="Review.SupportiveEvidence" disabled />1
           <input type="radio" value=2.00 asp-for="Review.SupportiveEvidence" disabled />2
           <input type="radio" value=3.00 asp-for="Review.SupportiveEvidence" disabled />3
           <input type="radio" value=4.00 asp-for="Review.SupportiveEvidence" disabled />4
           <input type="radio" value=5.00 asp-for="Review.SupportiveEvidence" disabled />5
           <span asp-validation-for="Review.SupportiveEvidence" class="text-danger"></span>
         </div>
          <div class="mb-3">
           <label class="control-label col-sm-3">Technical Quality:</label>
           <input type="radio" value=1.00 asp-for="Review.TechnicalQuality " disabled />1
           <input type="radio" value=2.00 asp-for="Review.TechnicalQuality" disabled />2
           <input type="radio" value=3.00 asp-for="Review.TechnicalQuality" disabled />3
           <input type="radio" value=4.00 asp-for="Review.TechnicalQuality" disabled />4
           <input type="radio" value=5.00 asp-for="Review.TechnicalQuality" disabled />5
           <span asp-validation-for="Review.TechnicalQuality" class="text-danger"></span>
         </div>
          <div class="mb-3">
           <label class="control-label col-sm-3">Scope Of Coverage:</label>
           <input type="radio" value=1.00 asp-for= "Review.ScopeOfCoverage" disabled />1
           <input type="radio" value=2.00 asp-for= "Review.ScopeOfCoverage" disabled />2
           <input type="radio" value=3.00 asp-for= "Review.ScopeOfCoverage" disabled />3
           <input type="radio" value=4.00 asp-for= "Review.ScopeOfCoverage" disabled />4
           <input type="radio" value=5.00 asp-for= "Review.ScopeOfCoverage" disabled />5
           <span asp-validation-for="Review.ScopeOfCoverage" class="text-danger"></span>
         </div>
          <div class="mb-3">
