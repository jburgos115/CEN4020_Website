@page
@model CEN4020_Website.Pages.PaperReview.AddReviewModel
@{
}
<form method="post" class="form-horizontal">
	<div class=" border p-3 mt-4">
		<div class="row pb-2">
			<h2 class="text-primary pl-3">Add a Review</h2>
			<hr />
		</div>
      
		<div class="mb-3">
           <label class="control-label col-sm-3">Paper ID:</label>
           <input asp-for="Review.PaperID" type="text" size="50"/>
           <span asp-validation-for="Review.PaperID" class="text-danger"></span>
        </div>
       
         <div class="mb-3">
           <label class="control-label col-sm-3">Reviewer ID:</label>
           <input asp-for="Review.ReviewerID" type="text" size="50" />
           <span asp-validation-for="Review.ReviewerID" class="text-danger"></span>
         </div>
          <div class="mb-3">
           <label class="control-label col-sm-3">Appropriateness Of Topic:</label>
           <input type="radio" value="1" asp-for="@Model.Review.AppropriatenessOfTopic" />1
           <input type="radio" value="2" asp-for="@Model.Review.AppropriatenessOfTopic" />2
           <input type="radio" value="3" asp-for="@Model.Review.AppropriatenessOfTopic" />3
           <input type="radio" value="4" asp-for="@Model.Review.AppropriatenessOfTopic"/>4
           <input type="radio" value="5" asp-for="@Model.Review.AppropriatenessOfTopic" />5
           <span asp-validation-for="Review.AppropriatenessOfTopic" class="text-danger"></span>
         </div>
          <div class="mb-3">
           <label class="control-label col-sm-3">Timeliness Of Topic:</label>
           <input type="radio" value="1" asp-for="Review.TimelinessOfTopic" />1
           <input type="radio" value="2" asp-for="Review.TimelinessOfTopic " />2
           <input type="radio" value="3" asp-for="Review.TimelinessOfTopic " />3
           <input type="radio" value="4" asp-for="Review.TimelinessOfTopic " />4
           <input type="radio" value="5" asp-for="Review.TimelinessOfTopic" />5
           <span asp-validation-for="Review.TimelinessOfTopic" class="text-danger"></span>
         </div>
          <div class="mb-3">
           <label class="control-label col-sm-3">Supportive Evidence:</label>
           <input type="radio" value="1" asp-for="Review.SupportiveEvidence" />1
           <input type="radio" value="2" asp-for="Review.SupportiveEvidence" />2
           <input type="radio" value="3" asp-for="Review.SupportiveEvidence" />3
           <input type="radio" value="4" asp-for="Review.SupportiveEvidence" />4
           <input type="radio" value="5" asp-for="Review.SupportiveEvidence" />5
           <span asp-validation-for="Review.SupportiveEvidence" class="text-danger"></span>
         </div>
          <div class="mb-3">
           <label class="control-label col-sm-3">Technical Quality:</label>
           <input type="radio" value="1" asp-for="Review.TechnicalQuality " />1
           <input type="radio" value="2" asp-for="Review.TechnicalQuality" />2
           <input type="radio" value="3" asp-for="Review.TechnicalQuality" />3
           <input type="radio" value="4" asp-for="Review.TechnicalQuality" />4
           <input type="radio" value="5" asp-for="Review.TechnicalQuality" />5
           <span asp-validation-for="Review.TechnicalQuality" class="text-danger"></span>
         </div>
          <div class="mb-3">
           <label class="control-label col-sm-3">Scope Of Coverage:</label>
           <input type="radio" value="1" asp-for= "Review.ScopeOfCoverage" />1
           <input type="radio" value="2" asp-for= "Review.ScopeOfCoverage" />2
           <input type="radio" value="3" asp-for= "Review.ScopeOfCoverage" />3
           <input type="radio" value="4" asp-for= "Review.ScopeOfCoverage" />4
           <input type="radio" value="5" asp-for= "Review.ScopeOfCoverage" />5
           <span asp-validation-for="Review.ScopeOfCoverage" class="text-danger"></span>
         </div>
          <div class="mb-3">
           <label class="control-label col-sm-3">Citation Of Previous Work:</label>
           <input type="radio" value="1" asp-for="Review.CitationOfPreviousWork" />1
