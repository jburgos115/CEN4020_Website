           <input type="radio" value="1" asp-for="Review.Originality" />1
           <input type="radio" value="2" asp-for="Review.Originality" />2
           <input type="radio" value="3" asp-for="Review.Originality" />3
           <input type="radio" value="4" asp-for="Review.Originality" />4
           <input type="radio" value="5" asp-for="Review.Originality" />5
           <span asp-validation-for="Review.Originality" class="text-danger"></span>
         </div>
          <div class="mb-3">
           <label class="control-label col-sm-3">Content Comments:</label>
           <input asp-for="Review.ContentComments" type="text" size="50"  />
           <span asp-validation-for="Review.ContentComments" class="text-danger"></span>
         </div>
