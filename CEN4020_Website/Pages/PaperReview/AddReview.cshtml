           <input type="radio" value="2" asp-for="Review.Mechanics" />2
           <input type="radio" value="3" asp-for="Review.Mechanics" />3
           <input type="radio" value="4" asp-for="Review.Mechanics" />4
           <input type="radio" value="5" asp-for="Review.Mechanics" />5
           <span asp-validation-for="Review.Mechanics" class="text-danger"></span>
         </div>
